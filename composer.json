{
    "name": "crowd2fund/finblocks-php-sdk",
    "description": "This is the Crowd2Fund's PHP SDK for FinBlocks",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "David Garcia",
            "email": "me@davidgarcia.cat",
            "role": "Developer"
        },
        {
            "name": "David Garcia",
            "email": "david.garcia@crowd2fund.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "FinBlocks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FinBlocks\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "monolog/monolog": "^1.0",
        "webmozart/assert": "^1.0"
    },
    "require-dev": {
        "nyholm/nsa": "^1.1",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^9.3.0",
        "rector/rector": "^1.2"
    },
    "config": {
        "sort-packages": true
    }
}
